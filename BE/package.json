  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "tsx --watch src/index.ts",
    "dev_test": "nodemon --watch src --ext ts --exec \"npx ts-node src/core/rbac/rbac-test.ts\"",
    "dev": "concurrently \"tsc --watch\" \"nodemon --exec tsx src/index.ts\"",
    "dev:demo": "nodemon --exec tsx src/transports/express/demo-server.ts",
    "demo:new": "tsx src/transports/expressServer.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "generator:alias": "npx ts-node src/generators/alias/batchGenerator.ts full",
    "generator:response": "npx ts-node src/generators/response/batch-generator.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
